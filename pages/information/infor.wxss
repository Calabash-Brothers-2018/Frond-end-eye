page{
  background-color:whitesmoke;
  height: 100%; 
}

.image{
  margin-top: 10px;
  display: flex;  
  flex-direction: column;  
  align-items: center;  
}

.photo{
  border-radius: 20%; 
}

.information{  
  background-color:white; 
  width: 300px;  
  border: solid 1px black; 
  border-radius: 5%;
  -webkit-box-align: center;  
  -ms-flex-align: center;  
  align-items: center;   
  margin-top: 20px;
  margin-left: 35px;
}

.place{
  display: flex;
  flex-direction: row;
  justify-content: center;
  border-bottom: solid 1px black;
  font-size: 20px;
}